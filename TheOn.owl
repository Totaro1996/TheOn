    <!-- http://www.TheOn.org#has_epithet -->

    <owl:DatatypeProperty rdf:about="http://www.TheOn.org#has_epithet">
        <rdfs:domain rdf:resource="http://www.TheOn.org#Divine_Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.TheOn.org#is_induced -->

    <owl:DatatypeProperty rdf:about="http://www.TheOn.org#is_induced">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#UnintentionalProcess"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/lso/intro/beta201901#R44_has_wording -->

    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta201901#R44_has_wording">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.TheOn.org#Animal -->

    <owl:Class rdf:about="http://www.TheOn.org#Animal">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:comment>The category referring to animals</rdfs:comment>
        <rdfs:label>Animal</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Chorus -->

    <owl:Class rdf:about="http://www.TheOn.org#Chorus">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#Dramatic_mechanism"/>
        <rdfs:comment>The class Chorus refers to the homogeneous, non-individualised group of performers, who comment with a collective voice on the dramatic action in the context of ancient greek tragedy, comedy or satyr plays.</rdfs:comment>
        <rdfs:label>Chorus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Deus_Ex_Machina -->

    <owl:Class rdf:about="http://www.TheOn.org#Deus_Ex_Machina">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#Dramatic_mechanism"/>
        <rdfs:comment>Mechanism used in ancient Greek and Roman drama which consists of a person or thing that appears or is introduced into a situation suddenly and unexpectedly and provides an artificial or contrived solution to an apparently insoluble difficulty.</rdfs:comment>
        <rdfs:label>Deus_ex_machina</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Divine_Entity -->

    <owl:Class rdf:about="http://www.TheOn.org#Divine_Entity">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:comment>An entity that is presented in the text as divine or sacred in some form, thus has for the most part divine characteristics.</rdfs:comment>
        <rdfs:label>Divine_entity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Dramatic_mechanism -->

    <owl:Class rdf:about="http://www.TheOn.org#Dramatic_mechanism">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lso/intro/beta201901#INT_GenreSpecifics"/>
        <rdfs:comment>The class Dramatic_mechanism refers to all the mechanisms performed in ancient dramas which have a physical, not only textual,correspondence.</rdfs:comment>
        <rdfs:label>Dramatic_mechanism</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Entreaty -->

    <owl:Class rdf:about="http://www.TheOn.org#Entreaty">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#RitualAction"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Epithet -->

    <owl:Class rdf:about="http://www.TheOn.org#Epithet">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lso/intro/beta201901#INT_Figure_of_Speech"/>
        <rdfs:comment>An epithet is a byname, or a descriptive term (word or phrase), accompanying or occurring in place of a name and having entered common usage. (e.g Pallas Athena)</rdfs:comment>
        <rdfs:label>Epithet</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Epithet_action -->

    <owl:Class rdf:about="http://www.TheOn.org#Epithet_action">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#Epithet"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Formal_Expression -->

    <owl:Class rdf:about="http://www.TheOn.org#Formal_Expression">
        <owl:disjointWith rdf:resource="http://www.purl.org/drammar#DramaEntity"/>
        <rdfs:comment>An Information Entity can be interpreted at two different levels, the one from the point of view of the content and the one from the point of view of the linguistic and rhetorical features. In this sense, the formal  content of a text is composed of features that can be analysed only from the linguistic point of view.</rdfs:comment>
        <rdfs:label>Formal_Expression</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#GenericAction -->

    <owl:Class rdf:about="http://www.TheOn.org#GenericAction">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#Action"/>
        <rdfs:comment>The class GenericAction refers to all the actions which are not ritual</rdfs:comment>
        <rdfs:label>GenericAction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Generic_UnintentialProcess -->

    <owl:Class rdf:about="http://www.TheOn.org#Generic_UnintentialProcess">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#UnintentionalProcess"/>
        <rdfs:comment>A Generic_UnintentionalProcess is a process with the property isintentional that values False and implies a divine influence which is generic thus it does not specify a specific way of acting,</rdfs:comment>
        <rdfs:label>Generic_UnintentionalProcess</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Human_Entity -->

    <owl:Class rdf:about="http://www.TheOn.org#Human_Entity">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:comment>An entity that is presented in the text as human and that has for the most part human characteristics</rdfs:comment>
        <rdfs:label>Human_Entity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Hybris -->

    <owl:Class rdf:about="http://www.TheOn.org#Hybris">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lso/intro/beta201901#INT_Topos"/>
        <rdfs:comment>This class refers to an excess of ambition, pride, etc ultimately causing the transgressor&apos;s ruin in Greek tragedy</rdfs:comment>
        <rdfs:label>Hybris</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Invocation -->

    <owl:Class rdf:about="http://www.TheOn.org#Invocation">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#RitualAction"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Mental_UnintentionalProcess -->

    <owl:Class rdf:about="http://www.TheOn.org#Mental_UnintentionalProcess">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#UnintentionalProcess"/>
        <rdfs:comment>A Mental_UnintentionalProcess is a process with the property isintentional that values False and implies a divine influence which includes non-physical actions (e.g. suggest thoughts)</rdfs:comment>
        <rdfs:label>Mental_UnintentionalProcess</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Metrical_verses -->

    <owl:Class rdf:about="http://www.TheOn.org#Metrical_verses">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lso/intro/currentbeta#INT10_FormalFeature"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Physical_UnintentionalProcess -->

    <owl:Class rdf:about="http://www.TheOn.org#Physical_UnintentionalProcess">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#UnintentionalProcess"/>
        <rdfs:comment>A Physical_UnintentionalProcess is a process with the property isintentional that values False and implies a divine influence which includes physical actions (e.g. throwing a spear, offering prizes or threatening punishment)</rdfs:comment>
        <rdfs:label>Physical_UnintentionalProcess</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Physical_feature -->

    <owl:Class rdf:about="http://www.TheOn.org#Physical_feature">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#Epithet"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Prayer -->

    <owl:Class rdf:about="http://www.TheOn.org#Prayer">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#RitualAction"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Punishment -->

    <owl:Class rdf:about="http://www.TheOn.org#Punishment">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#GenericAction"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Revenge -->

    <owl:Class rdf:about="http://www.TheOn.org#Revenge">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#GenericAction"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#RitualAction -->

    <owl:Class rdf:about="http://www.TheOn.org#RitualAction">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#Action"/>
        <rdfs:comment>The class RitualAction refers to all the actions describing the ways in which humans turn to divine entities in the tragedies by means of rituals.</rdfs:comment>
        <rdfs:label>RitualAction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Sacrifice -->

    <owl:Class rdf:about="http://www.TheOn.org#Sacrifice">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#RitualAction"/>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Specific_UnintentionalProcess -->

    <owl:Class rdf:about="http://www.TheOn.org#Specific_UnintentionalProcess">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#UnintentionalProcess"/>
        <rdfs:comment>A Specific_UnintentionalProcess is a process with the property isintentional that values False and implies a divine influence which is specific thus it does specify a specific way of acting</rdfs:comment>
        <rdfs:label>Specific_UnintentionalProcess</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Vow -->

    <owl:Class rdf:about="http://www.TheOn.org#Vow">
        <rdfs:subClassOf rdf:resource="http://www.TheOn.org#RitualAction"/>
    </owl:Class>
    


    <!-- http://www.cadmos.cirma.unito.it/drammar/2012/4/drammar.owl#Agent -->

    <owl:Class rdf:about="http://www.cadmos.cirma.unito.it/drammar/2012/4/drammar.owl#Agent"/>
    


