     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:TheOn="http://www.TheOn.org#"
     xmlns:beta201901="https://w3id.org/lso/intro/beta201901#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://www.TheOn.org">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl"/>
        <owl:imports rdf:resource="http://www.purl.org/drammar/0.9"/>
        <owl:imports rdf:resource="https://w3id.org/lso/intro/beta201901"/>
