    <!-- http://www.TheOn.org#Killing_of_Agamemnon -->

    <owl:NamedIndividual rdf:about="http://www.TheOn.org#Killing_of_Agamemnon">
        <rdf:type rdf:resource="http://www.TheOn.org#Mental_UnintentionalProcess"/>
