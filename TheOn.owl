    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.TheOn.org#affects -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#affects">
        <rdfs:domain rdf:resource="http://www.TheOn.org#GenericAction"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#contains_literary_motif -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#contains_literary_motif">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta201901#INT9_SemanticFeature"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/currentbeta#INT3_IntertextualRelationship"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta201901#INT_Motif"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#has_concept -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#has_concept">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#Meaning"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Process"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#has_subject -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#has_subject">
        <owl:inverseOf rdf:resource="http://www.TheOn.org#is_subject_of"/>
        <rdfs:domain rdf:resource="http://www.TheOn.org#Deus_Ex_Machina"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Divine_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#has_textual_concretization -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#has_textual_concretization">
        <rdfs:domain rdf:resource="http://www.TheOn.org#Dramatic_mechanism"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta201901#INT8_RhetoricalFeature"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/currentbeta#INT10_FormalFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_caused_by -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_caused_by">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#UnintentionalProcess"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Divine_Entity"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Human_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_characterization_of -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_characterization_of">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta201901#INT8_RhetoricalFeature"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta201901#INT9_SemanticFeature"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/currentbeta#INT10_FormalFeature"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/currentbeta#INT3_IntertextualRelationship"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Divine_Entity"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Human_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_interepreted_as -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_interepreted_as">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta201901#INT1_TextPassage"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Formal_Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_object_of -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_object_of">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#DramaEntity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#Meaning"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_prerogative_of -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_prerogative_of">
        <rdfs:domain rdf:resource="http://www.TheOn.org#RitualAction"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Human_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_presented_as -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_presented_as">
        <rdfs:domain rdf:resource="http://www.cadmos.cirma.unito.it/drammar/2012/4/drammar.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Animal"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Divine_Entity"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Human_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_represented_by -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_represented_by">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Process"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Formal_Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_subject_of -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_subject_of">
        <rdfs:domain rdf:resource="http://www.TheOn.org#Divine_Entity"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Deus_Ex_Machina"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#is_textual_concretization_of -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#is_textual_concretization_of">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta201901#INT8_RhetoricalFeature"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/currentbeta#INT10_FormalFeature"/>
        <rdfs:range rdf:resource="http://www.TheOn.org#Dramatic_mechanism"/>
    </owl:ObjectProperty>
    


    <!-- http://www.TheOn.org#takes_part_in -->

    <owl:ObjectProperty rdf:about="http://www.TheOn.org#takes_part_in">
        <rdfs:domain rdf:resource="http://www.cadmos.cirma.unito.it/drammar/2012/4/drammar.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Process"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#denotes -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#denotes">
        <rdfs:range rdf:resource="http://www.purl.org/drammar#DramaEntity"/>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#hasInterpretant -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#hasInterpretant">
        <rdfs:range rdf:resource="http://www.TheOn.org#Formal_Expression"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/semiotics.owl#Meaning"/>
    </rdf:Description>
    


    <!-- https://w3id.org/lso/intro/beta201901#R18_shows_actualization -->

    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta201901#R18_shows_actualization">
        <rdfs:domain rdf:resource="http://www.TheOn.org#Formal_Expression"/>
    </rdf:Description>
    


    <!-- https://w3id.org/lso/intro/beta201901#R4_defines -->

    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta201901#R4_defines">
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta201901#INT8_RhetoricalFeature"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta201901#INT9_SemanticFeature"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/currentbeta#INT10_FormalFeature"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/currentbeta#INT3_IntertextualRelationship"/>
    </rdf:Description>
    


