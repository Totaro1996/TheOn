    <!-- http://www.TheOn.org#Revenge1 -->

    <owl:NamedIndividual rdf:about="http://www.TheOn.org#Revenge1">
        <rdf:type rdf:resource="http://www.TheOn.org#Revenge"/>
        <affects rdf:resource="http://www.TheOn.org#Agamemnon"/>
        <beta201901:R44_has_wording rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;For, in her pity, holy Artemis is angry at the winged hounds of her father, for they sacrifice a wretched timorous thing, together with her young, before she has brought them forth. An abomination to her is the eagles&apos; feast.”</beta201901:R44_has_wording>
